aturedPosts: function QFeaturedPosts() {\n      return import(\"../queries/QFeaturedProd.vue\");\n    }\n  },\n  filters: {\n    strippedContent: function strippedContent(string) {\n      return string.replace(/<\\/?[^>]+>/gi, \" \");\n    }\n  },\n  computed: {\n    affiliate: function affiliate() {\n      return '&tag=newdev-21';\n    },\n    reviews: function reviews() {\n      return '#customerReviews';\n    }\n  }\n};",{"version":3,"sources":["Index.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA2JA,eAAA;AACA,EAAA,QAAA,EAAA;AACA,IAAA,KAAA,EAAA;AADA,GADA;AAKA,EAAA,OAAA,EAAA;AACA,IAAA,MADA,kBACA,MADA,EACA;AAAA,UACA,GADA,GACA,MADA,CACA,GADA;AAEA,aAAA,MAAA,CAAA,GAAA,CAAA,CAAA,KAAA,MAAA,CAAA,GAAA,CAAA,CAAA,CAAA;AACA;AAJA,GALA;AAYA,EAAA,UAAA,EAAA;AACA,IAAA,cAAA,EAAA;AAAA,aAAA,OAAA,8BAAA,CAAA;AAAA;AADA,GAZA;AAeA,EAAA,OAAA,EAAA;AACA,IAAA,eAAA,EAAA,yBAAA,MAAA,EAAA;AACA,aAAA,MAAA,CAAA,OAAA,CAAA,cAAA,EAAA,GAAA,CAAA;AACA;AAHA,GAfA;AAoBA,EAAA,QAAA,EAAA;AACA,IAAA,SAAA,EAAA,qBAAA;AACA,aAAA,gBAAA;AACA,KAHA;AAIA,IAAA,OAAA,EAAA,mBAAA;AACA,aAAA,kBAAA;AACA;AANA;AApBA,CAAA","sourcesContent":["<template>\r\n  <Layout>\r\n    <section class=\"section-intro padding-y-sm\">\r\n      <div class=\"container fludid\">\r\n        <div class=\"intro-banner-wrap\">\r\n          <g-image src=\"~/assets/images/banners/1.jpg\" class=\"img-fluid rounded\"></g-image>\r\n        </div>\r\n      </div>\r\n      <!-- container //  -->\r\n    </section>\r\n    <section class=\"section-content\">\r\n      <div class=\"container\">\r\n        <header class=\"section-heading\">\r\n          <a href=\"/product-list/\" class=\"btn btn-outline-primary float-right\">Vedi tutti</a>\r\n          <h3 class=\"section-title\">Prodotti del momento</h3>\r\n        </header>\r\n        <!-- sect-heading -->\r\n\r\n        <div class=\"row\">\r\n          <div class=\"col-md-3\" v-for=\"pro in $page.allProduct.edges\" :key=\"pro.id\">\r\n            <div href=\"#\" class=\"card card-product-grid\">\r\n             \r\n                <g-link :to=\"pro.node.path\" class=\"img-wrap\">\r\n                  <g-image :src=\"getSrc(pro.node.images)\" />\r\n                </g-link>\r\n              \r\n              <figcaption class=\"info-wrap\">\r\n                <span>\r\n                  <g-link :to=\"pro.node.path\" class=\"title text-truncate\">{{ pro.node.title }}</g-link>\r\n                </span>\r\n\r\n                <div class=\"rating-wrap\"></div>\r\n\r\n                <!-- price-wrap.// -->\r\n              </figcaption>\r\n              <figcaption class=\"info-wrap\">\r\n                <a href=\"#\" class=\"btn btn-block btn-success\">\r\n                  <i class=\"fas fa-shopping-cart\"></i>\r\n                  {{ pro.node.listPrice }}\r\n                </a>\r\n              </figcaption>\r\n            </div>\r\n          </div>\r\n          <!-- col.// -->\r\n        </div>\r\n        <!-- row.// -->\r\n      </div>\r\n\r\n      <div class=\"container mb-3\">\r\n        <header class=\"section-heading\">\r\n          <h3 class=\"section-title\">Nuovi arrivi</h3>\r\n        </header>\r\n      </div>\r\n\r\n      <div class=\"container\">\r\n        <div class=\"row\">\r\n          <!-- START NEW PRODUCTS LIST -->\r\n          <QFeaturedPosts v-slot=\"{ posts: featured_posts }\">\r\n            <main class=\"col-md-12\" v-for=\"fpost in featured_posts\" :key=\"fpost.id\">\r\n              <article class=\"card card-product-list\">\r\n                <div class=\"row no-gutters\">\r\n                  <aside class=\"col-md-3\">\r\n                    <a href=\"#\" class=\"img-wrap\">\r\n                      <span class=\"badge badge-danger\">NEW</span>\r\n                      <img :src=\"getSrc(fpost.images)\" />\r\n                    </a>\r\n                  </aside>\r\n                  <div class=\"col-md-6\">\r\n                    <div class=\"info-main\">\r\n                      <g-link :to=\"fpost.path\" class=\"h5 title\">{{fpost.title}}</g-link>\r\n                      <!-- <div class=\"rating-wrap mb-3\">\r\n                        <ul class=\"rating-stars\">\r\n                          <li style=\"width:80%\" class=\"stars-active\">\r\n                            <i class=\"fa fa-star\"></i>\r\n                            <i class=\"fa fa-star\"></i>\r\n                            <i class=\"fa fa-star\"></i>\r\n                            <i class=\"fa fa-star\"></i>\r\n                            <i class=\"fa fa-star\"></i>\r\n                          </li>\r\n                          <li>\r\n                            <i class=\"fa fa-star\"></i>\r\n                            <i class=\"fa fa-star\"></i>\r\n                            <i class=\"fa fa-star\"></i>\r\n                            <i class=\"fa fa-star\"></i>\r\n                            <i class=\"fa fa-star\"></i>\r\n                          </li>\r\n                        </ul>\r\n                        <div class=\"label-rating\">7/10</div>\r\n                      </div> -->\r\n                      <!-- rating-wrap.// -->\r\n\r\n                      <p>\r\n                        {{fpost.description.substring(0,200)+\"...\" | strippedContent}}\r\n                        <g-link :to=\"fpost.path\">+ info</g-link>\r\n                      </p>\r\n                    </div>\r\n                    <!-- info-main.// -->\r\n                  </div>\r\n                  <aside class=\"col-sm-3\">\r\n                    <div class=\"info-aside\">\r\n                      <div class=\"price-wrap\">\r\n                        <span class=\"price h5\">{{fpost.listPrice}}</span>\r\n                        <!-- <del class=\"price-old\">{{fpost.price}}</del> -->\r\n                      </div>\r\n                      <!-- info-price-detail // -->\r\n                      <p class=\"text-success\">Spedizione gratuita</p>\r\n                      <br />\r\n                      <p>\r\n                        <a :href=\"fpost.uRL+affiliate\" class=\"btn btn-success btn-block\">Acquista</a>\r\n                        <a :href=\"fpost.uRL+affiliate+reviews\" class=\"btn btn-light btn-block\">\r\n                          <i class=\"fa fa-heart\"></i>\r\n                          <span class=\"text\">Recensioni</span>\r\n                        </a>\r\n                        <!-- <span>{{fpost.uRL}}</span> -->\r\n                      </p>\r\n                    </div>\r\n                    <!-- info-aside.// -->\r\n                  </aside>\r\n                </div>\r\n              </article>\r\n            </main>\r\n          </QFeaturedPosts>\r\n          <!-- END NEW PRODUCTS LIST -->\r\n        </div>\r\n      </div>\r\n    </section>\r\n    \r\n  </Layout>\r\n</template>\r\n\r\n<style lang=\"scss\">\r\n@import \"../assets/scss/fonts\";\r\n</style>\r\n\r\n<page-query>\r\nquery {\r\n  allProduct (skip: 20 limit: 4) {\r\n    edges {\r\n      node {\r\n        id\r\n        title\r\n        price\r\n        listPrice\r\n        path\r\n        uRL       \r\n        images {\r\n          uRL\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n</page-query>\r\n\r\n<script>\r\nexport default {\r\n  metaInfo: {\r\n    title: \"Hello, world!\"\r\n  },\r\n\r\n  methods: {\r\n    getSrc(images) {\r\n      const { uRL } = images;\r\n      return images.uRL[1] || images.uRL[0];\r\n    }\r\n  },\r\n\r\n  components: {\r\n    QFeaturedPosts: () => import(\"../queries/QFeaturedProd.vue\")\r\n  },\r\n  filters: {\r\n    strippedContent: function(string) {\r\n      return string.replace(/<\\/?[^>]+>/gi, \" \");\r\n    }\r\n  },\r\n  computed: {\r\n    affiliate: function() {\r\n      return '&tag=newdev-21';\r\n    },\r\n    reviews: function() {\r\n      return '#customerReviews';\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n"],"sourceRoot":"src/pages"}]}