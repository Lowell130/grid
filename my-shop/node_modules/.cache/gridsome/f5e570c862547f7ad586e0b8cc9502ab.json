\r\n      <div class=\"row pb-4\">\r\n        <div class=\"col-lg-4 mb-4\" v-for=\"pro in $page.allProduct.edges\" :key=\"pro.id\">\r\n         \r\n\r\n          <div class=\"card\">\r\n            <g-image alt=\"Card image cap\" class=\"card-img-top\" :src=\"getSrc(pro)\" />\r\n            <div class=\"card-body\">\r\n              <h5 class=\"card-title text-truncate\">{{ pro.node.title }}</h5>\r\n              <p\r\n                class=\"card-text\"\r\n              >Some quick example text to build on the card title and make up the bulk of the card's content.</p>\r\n              <a href=\"#\" class=\"btn btn-primary\">Go somewhere</a>\r\n            </div>\r\n          </div>\r\n         \r\n        </div>\r\n      </div>\r\n\r\n      <hr class=\"featurette-divider\" />\r\n    </div>-->\r\n  </Layout>\r\n</template>\r\n\r\n\r\n\r\n<style lang=\"scss\">\r\n@import '../assets/scss/fonts';\r\n// @import '../assets/scss/bootstrap.scss';\r\n// @import '../assets/scss/main.scss';\r\n// @import '../assets/fonts/fontawesome/css/all.css';\r\n\r\n</style>\r\n\r\n\r\n\r\n<page-query>\r\nquery {\r\n  allProduct (limit: 16) {\r\n    edges {\r\n      node {\r\n        id\r\n        title\r\n        price\r\n        listPrice\r\n        path\r\n        images {\r\n          uRL\r\n        }\r\n      }\r\n    }\r\n  }\r\n\r\n}\r\n</page-query>\r\n\r\n\r\n\r\n\r\n<script>\r\nexport default {\r\n  metaInfo: {\r\n    title: \"Hello, world!\"\r\n  },\r\n\r\n  methods: {\r\n    getSrc(pro) {\r\n      const { uRL } = pro.node.images;\r\n\r\n      return uRL[1] || uRL[0];\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n"],"sourceRoot":"src/pages"}]}