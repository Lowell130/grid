lters: {\n    strippedContent: function strippedContent(string) {\n      return string.replace(/<\\/?[^>]+>/gi, \" \");\n    }\n  }\n};",{"version":3,"sources":["Product-list.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAsGA,SAAA,KAAA,QAAA,UAAA;AACA,eAAA;AACA,EAAA,QAAA,EAAA;AACA,IAAA,KAAA,EAAA;AADA,GADA;AAIA,EAAA,UAAA,EAAA;AACA,IAAA,KAAA,EAAA;AADA,GAJA;AAOA,EAAA,OAAA,EAAA;AACA,IAAA,MADA,kBACA,MADA,EACA;AAAA,UACA,GADA,GACA,MADA,CACA,GADA;AAEA,aAAA,MAAA,CAAA,GAAA,CAAA,CAAA,KAAA,MAAA,CAAA,GAAA,CAAA,CAAA,CAAA;AACA;AAJA,GAPA;AAaA,EAAA,QAAA,EAAA;AACA,IAAA,SAAA,EAAA,qBAAA;AACA,aAAA,gBAAA;AACA,KAHA;AAIA,IAAA,UAAA,EAAA,sBAAA;AACA,aAAA,kBAAA;AACA;AANA,GAbA;AAqBA,EAAA,OAAA,EAAA;AACA,IAAA,eAAA,EAAA,yBAAA,MAAA,EAAA;AACA,aAAA,MAAA,CAAA,OAAA,CAAA,cAAA,EAAA,GAAA,CAAA;AACA;AAHA;AArBA,CAAA","sourcesContent":["<template>\r\n  <Layout>\r\n    <div class=\"container top pb-4\">\r\n      <h1>Tutti i prodotti</h1>\r\n      <p>In questa pagina troverai tutti i prodotti presenti sul sito</p>\r\n      <!-- <ul>\r\n      <li v-for=\"{ node: product } in $page.allProduct.edges\" :key=\"product.id\">\r\n        <g-link :to=\"product.path\">{{ product.title }}</g-link>\r\n      </li>\r\n    </ul>\r\n      <Pager :info=\"$page.allProduct.pageInfo\"/>-->\r\n    </div>\r\n\r\n    <section class=\"section-content padding-y bg\">\r\n      <div class=\"container\">\r\n        <div class=\"row\">\r\n          <div class=\"col-12\">\r\n            <!-- ============================ COMPONENT 2  ================================= -->\r\n            <div\r\n              class=\"card card-product-list\"\r\n              v-for=\"{ node: product } in $page.allProduct.edges\"\r\n              :key=\"product.id\"\r\n            >\r\n              <div class=\"card-body\">\r\n                <div class=\"row\">\r\n                  <aside class=\"col-sm-3\">\r\n                    <g-link :to=\"product.path\" class=\"img-wrap\">\r\n                      <g-image :src=\"getSrc(product.images)\" :alt=\"product.title\" />\r\n                    </g-link>\r\n                  </aside>\r\n                  <!-- col.// -->\r\n                  <article class=\"col-sm-6\">\r\n                    <g-link class=\"title mt-2 h5\" :to=\"product.path\">{{ product.title }}</g-link>\r\n                    <div class=\"rating-wrap mb-2\">\r\n                      <ul class=\"rating-stars\">\r\n                        <li style=\"width:100%\" class=\"stars-active\">\r\n                          <i class=\"fa fa-star\"></i>\r\n                        </li>\r\n                        <li>\r\n                          <i class=\"fa fa-star\"></i>\r\n                        </li>\r\n                      </ul>\r\n                      <small class=\"label-rating text-muted\">\r\n                        <a :href=\"product.uRL+reviewsAmz\">Leggi recensioni</a>\r\n                      </small>\r\n                      <small class=\"label-rating text-success\" v-if=\"product.discount !== '0%'\">\r\n                        <i class=\"fa fa-clipboard-check\"></i>&nbsp;\r\n                        <span>RISPARMI {{product.discount}}</span>\r\n                      </small>\r\n                    </div>\r\n                    <p>\r\n                      {{product.description.substring(0,100)+\"...\" | strippedContent}}\r\n                      <g-link :to=\"product.path\">+ info</g-link>\r\n                    </p>\r\n                  </article>\r\n                  <!-- col.// -->\r\n                  <div class=\"info-aside col-md-3\">\r\n                    <div class=\"price-wrap text-center\">\r\n                      <span class=\"price h5\">{{ product.price }}€</span>\r\n                      <del class=\"price-old\" v-if=\"product.discount !== '0%'\">{{ product.listPrice }}€</del>\r\n                    </div>\r\n                   \r\n                    <p>\r\n                      <a\r\n                        :href=\"product.uRL+affiliate\"\r\n                        class=\"btn btn-success btn-lg btn-block\"\r\n                      >Acquista</a>\r\n                   \r\n                    </p>\r\n                             <p>\r\n                        <a :href=\"product.uRL+affiliate+reviews\" class=\"btn btn-warning btn-lg btn-block mt-1\">\r\n                          <i class=\"fa fa-heart\"></i>\r\n                          <span class=\"text\">Recensioni</span>\r\n                        </a>\r\n                     \r\n                      </p>\r\n                  </div>\r\n                </div>\r\n                <!-- row.// -->\r\n              </div>\r\n              <!-- card-body .// -->\r\n            </div>\r\n            <!-- ============================ COMPONENT 2  END .// ================================= -->\r\n          </div>\r\n          <!-- col.// -->\r\n        </div>\r\n\r\n        <div class=\"contaier\">\r\n          <Pager :info=\"$page.allProduct.pageInfo\" linkClass=\"pager__link\" class=\"pager\" />\r\n        </div>\r\n      </div>\r\n      <!-- container .//  -->\r\n    </section>\r\n  </Layout>\r\n</template>\r\n\r\n\r\n\r\n\r\n\r\n\r\n<script>\r\nimport { Pager } from \"gridsome\";\r\nexport default {\r\n  metaInfo: {\r\n    title: \"Tutti i prodotti\"\r\n  },\r\n  components: {\r\n    Pager\r\n  },\r\n  methods: {\r\n    getSrc(images) {\r\n      const { uRL } = images;\r\n      return images.uRL[1] || images.uRL[0];\r\n    }\r\n  },\r\n  computed: {\r\n    affiliate: function() {\r\n      return \"&tag=newdev-21\";\r\n    },\r\n    reviewsAmz: function() {\r\n      return \"#customerReviews\";\r\n    }\r\n  },\r\n   filters: {\r\n     strippedContent: function(string) {\r\n       return string.replace(/<\\/?[^>]+>/gi, \" \");\r\n     }\r\n   }\r\n};\r\n</script>\r\n\r\n<page-query>\r\nquery AllProducts ($page: Int) {\r\n  allProduct (sortBy: \"discount\", order: DESC, perPage: 13, page: $page) @paginate {\r\n    pageInfo {\r\n      totalPages\r\n      currentPage\r\n    }\r\n    edges {\r\n      node {\r\n        id\r\n        title\r\n        path\r\n        price\r\n        uRL\r\n        listPrice\r\n        images {\r\n          uRL\r\n        }\r\n        description\r\n        discount\r\n      }\r\n    }\r\n  }\r\n}\r\n</page-query>\r\n\r\n\r\n\r\n<style lang=\"scss\">\r\n.card-product-list {\r\n  .info-aside {\r\n    border-left: 1px solid #e4e4e4;\r\n    @media (max-width: 768px) {\r\n      border-left: none;\r\n      .price-wrap {text-align: center}\r\n      .text-success {text-align: center}\r\n    }\r\n  }\r\n}\r\n\r\n\r\n\r\n.pager {\r\n  display: inline-block;\r\n  width: 100%;\r\n  text-align: center;\r\n\r\n  &__link {\r\n    color: var(--link-color);\r\n    text-align: center;\r\n    text-decoration: none;\r\n    padding: 0.5rem 1rem;\r\n\r\n    &:hover:not(.active) {\r\n      background-color: var(--bg-content-color);\r\n      border-radius: 5px;\r\n      color: var(--link-color);\r\n    }\r\n  }\r\n}\r\n\r\n.active {\r\n  background-color: var(--bg-content-color);\r\n  border-radius: 5px;\r\n  text-decoration: underline;\r\n}\r\n</style>"],"sourceRoot":"src/pages"}]}